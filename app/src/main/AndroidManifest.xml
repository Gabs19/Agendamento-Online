<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.agendamento_online">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name=".Paciente.Cadastro_Consulta"></activity>
        <activity android:name=".Paciente.Dados_Cadastrais" />
        <activity android:name=".Paciente.Reset_senha" />
        <activity android:name=".Paciente.Login_Paciente" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Paciente.Registro_Paciente" />



        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>