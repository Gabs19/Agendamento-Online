<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.agendamento_online">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Agendamento Online"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Paciente.Cadastro_Consulta" />
        <activity android:name=".Paciente.Dados_Cadastrais" />
        <activity android:name=".Paciente.Reset_senha" />
        <activity android:name=".Paciente.Login_Paciente" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Paciente.Registro_Paciente" />
        <activity android:name=".Medico.Inicio" />
        <activity android:name=".Medico.Login_Medico" />
        <activity android:name=".Medico.Registro_Medico" />
        <activity android:name=".Medico.Dados_Cadastrais_Medico" />
        <activity android:name=".Medico.Reset_senha_medico" />
        <activity android:name=".Medico.Home_medico" />
        <activity
            android:name=".Home"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>